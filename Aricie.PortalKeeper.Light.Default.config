      <KeeperRuleOfRequestEvent name="Detect Simultaneous Connections ">
        <Decription><![CDATA[This rule serves identifying cases of simultaneous connections of the same user account from different locations.By default, the new client is left through, the former client is automatically signed out and an event is logged.]]></Decription>
        <Enabled>false</Enabled>
        <Action>
          <Instances>
            <SubTypes>
              <string>Aricie.DNN.Modules.PortalKeeper.SendEmailAction`1[[Aricie.DNN.Modules.PortalKeeper.RequestEvent, Aricie.PortalKeeper]], Aricie.PortalKeeper</string>
              <string>Aricie.DNN.Modules.PortalKeeper.LogOffAction, Aricie.PortalKeeper</string>
            </SubTypes>
            <ArrayOfActionProviderSettingsOfRequestEvent>
              <ActionProviderSettingsOfRequestEvent xsi:type="SendEmailActionOfRequestEvent" name="Warn Ubiquist User by Email">
                <Decription><![CDATA[The user being connected several times is warned by email.]]></Decription>
                <Enabled>true</Enabled>
                <ProviderName>Send Email Action</ProviderName>
                <LifeCycleEvent>PreRequestHandlerExecute</LifeCycleEvent>
                <RunDurationVarName>ActionRunDuration</RunDurationVarName>
                <TokenizedText>
                  <Template><![CDATA[We have just detected that on the portal "[Context:DnnContext:Portal:PortalName]", your account "[Context:Items:ClientUser:Username]" was connected simultaneously from two distinct locations. The following locations were last recorded: Client IP n1:"[Context:Items:FirstClientSource]". Client IP n2:"[Context:Items:LastClientSource]". If you think that was an identity theft, please contact urgently the portal administrator.]]></Template>
                  <EnableTokenReplace>true</EnableTokenReplace>
                  <AddNewTokenSources>false</AddNewTokenSources>
                  <AdditionalTokenSource>
                    <StaticTokens />
                    <TokenVariables>
                      <Instances>
                        <SubTypes />
                        <ArrayOfVariableInfo />
                      </Instances>
                    </TokenVariables>
                    <CacheTokenReplace>false</CacheTokenReplace>
                    <TokenProviders />
                    <ConditionalTokens />
                  </AdditionalTokenSource>
                </TokenizedText>
                <EnableTokenReplace>true</EnableTokenReplace>
                <AdditionalTokenSource>
                  <StaticTokens />
                  <TokenVariables>
                    <Instances>
                      <SubTypes />
                      <ArrayOfVariableInfo />
                    </Instances>
                  </TokenVariables>
                  <CacheTokenReplace>false</CacheTokenReplace>
                  <TokenProviders />
                  <ConditionalTokens />
                </AdditionalTokenSource>
                <FromAddress />
                <ToAddresses>[Context:Items:ClientUser:Email]</ToAddresses>
                <EmailObject>Multiple Connection detected for [Context:Items:ClientUser:Username]</EmailObject>
                <EmailPriority>High</EmailPriority>
                <BodyFormat>Text</BodyFormat>
                <Encoding>Default</Encoding>
              </ActionProviderSettingsOfRequestEvent>
              <ActionProviderSettingsOfRequestEvent xsi:type="LogOffAction" name="Sign Original User Off">
