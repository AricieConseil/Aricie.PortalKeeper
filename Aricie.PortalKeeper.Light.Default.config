                    <ProcessTokens>
                      <Enabled>false</Enabled>
                    </ProcessTokens>
                    <AdditionalFilters />
                  </ExpressionFilter>
                </DiscriminationSource>
                <LockDuration d="01:00:00" />
                <NewConnectionsDontMatch>true</NewConnectionsDontMatch>
              </ConditionProviderSettingsOfRequestEvent>
            </ArrayOfConditionProviderSettingsOfRequestEvent>
          </Instances>
